{
  "checks": {
    "mesos": {
        "command": "/usr/local/bin/check-mesos.rb -s {{ mesos_master_list }} -m master",
        "interval": 10,
        "subscribers": [
            "{{ cluster_name }}-master"
        ]
    },
    "marathon": {
        "command": "/usr/local/bin/check-marathon.rb -s {{ marathon_host }}",
        "interval": 10,
        "subscribers": [
            "{{ cluster_name }}-master"
        ]
    }
  }
}